extends _global

block layoutCss
  link(rel='stylesheet', href='/layouts/core.min.css?#{cacheBreaker}')
  link(rel='stylesheet', href='https://s3.amazonaws.com/outrightmental/vendor/font-awesome/css/font-awesome.min.css')

block layoutNav
  div.navbar.navbar-default.navbar-fixed-top
    div.container
      div.navbar-header
        a.navbar-brand(href='/')
          img.navbar-logo(src='/media/mental-roger-white.png' alt=navbar-brand-label #{projectName})
        button.navbar-toggle.collapsed(data-toggle='collapse', data-target='.my-navbar-collapse')
          span.icon-bar
          span.icon-bar
          span.icon-bar
      div.navbar-collapse.my-navbar-collapse.collapse
        include common/nav

block layoutScripts